<?xml version="1.0" encoding="ASCII"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="language" nsURI="platform:/plugin/org.emoflon.ibex.tgg.core.language/model/Language.ecore#/"
    nsPrefix="org.emoflon.ibex.tgg.core.language">
  <eClassifiers xsi:type="ecore:EClass" name="TGG" eSuperTypes="#//basic/TGGNamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="src" upperBound="-1" eType="ecore:EClass ../../org.eclipse.emf.ecore/model/Ecore.ecore#//EPackage"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="trg" upperBound="-1" eType="ecore:EClass ../../org.eclipse.emf.ecore/model/Ecore.ecore#//EPackage"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="corr" eType="ecore:EClass ../../org.eclipse.emf.ecore/model/Ecore.ecore#//EPackage"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="rules" upperBound="-1"
        eType="#//TGGRule" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="constraintDefinitions"
        upperBound="-1" eType="#//csp/TGGAttributeConstraintDefinition" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TGGRule" eSuperTypes="#//basic/TGGNamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="refines" upperBound="-1"
        eType="#//TGGRule"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="complements" eType="#//TGGRule"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="nodes" upperBound="-1"
        eType="#//TGGRuleNode" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="edges" upperBound="-1"
        eType="#//TGGRuleEdge" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="attributeConditions" upperBound="-1"
        eType="#//csp/TGGAttributeConstraint" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="abstract" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TGGRuleElement" abstract="true" eSuperTypes="#//basic/TGGNamedElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="domainType" eType="#//DomainType"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="bindingType" eType="#//BindingType"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TGGRuleNode" eSuperTypes="#//TGGRuleElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="incomingEdges" upperBound="-1"
        eType="#//TGGRuleEdge" eOpposite="#//TGGRuleEdge/trgNode"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="outgoingEdges" upperBound="-1"
        eType="#//TGGRuleEdge" eOpposite="#//TGGRuleEdge/srcNode"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="ecore:EClass ../../org.eclipse.emf.ecore/model/Ecore.ecore#//EClass"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="incomingCorrsSource" upperBound="-1"
        eType="#//TGGRuleCorr" eOpposite="#//TGGRuleCorr/source"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="incomingCorrsTarget" upperBound="-1"
        eType="#//TGGRuleCorr" eOpposite="#//TGGRuleCorr/target"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="attrExpr" upperBound="-1"
        eType="#//inplaceAttributes/TGGInplaceAttributeExpression" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TGGRuleCorr" eSuperTypes="#//TGGRuleNode">
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" eType="#//TGGRuleNode"
        eOpposite="#//TGGRuleNode/incomingCorrsSource"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="#//TGGRuleNode"
        eOpposite="#//TGGRuleNode/incomingCorrsTarget"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TGGRuleEdge" eSuperTypes="#//TGGRuleElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="srcNode" eType="#//TGGRuleNode"
        eOpposite="#//TGGRuleNode/outgoingEdges"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="trgNode" eType="#//TGGRuleNode"
        eOpposite="#//TGGRuleNode/incomingEdges"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="ecore:EClass ../../org.eclipse.emf.ecore/model/Ecore.ecore#//EReference"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="DomainType">
    <eLiterals name="SRC" literal="SRC"/>
    <eLiterals name="TRG" value="1" literal="TRG"/>
    <eLiterals name="CORR" value="2" literal="CORR"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="BindingType">
    <eLiterals name="CONTEXT" literal="CONTEXT"/>
    <eLiterals name="CREATE" value="1" literal="CREATE"/>
    <eLiterals name="NEGATIVE" value="2" literal="NEGATIVE"/>
  </eClassifiers>
  <eSubpackages name="inplaceAttributes" nsURI="platform:/plugin/org.emoflon.ibex.tgg.core.language/model/Language.ecore#//inplaceAttributes"
      nsPrefix="org.emoflon.ibex.tgg.core.language.inplaceAttributes">
    <eClassifiers xsi:type="ecore:EClass" name="TGGInplaceAttributeExpression">
      <eStructuralFeatures xsi:type="ecore:EReference" name="attribute" lowerBound="1"
          eType="ecore:EClass ../../org.eclipse.emf.ecore/model/Ecore.ecore#//EAttribute"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="valueExpr" eType="#//basic/expressions/TGGExpression"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="operator" lowerBound="1"
          eType="#//inplaceAttributes/TGGAttributeConstraintOperators" defaultValueLiteral=""/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="TGGAttributeConstraintOperators">
      <eLiterals name="EQUAL" literal="EQUAL"/>
      <eLiterals name="UNEQUAL" value="1" literal="UNEQUAL"/>
      <eLiterals name="GR_EQUAL" value="2" literal="GR_EQUAL"/>
      <eLiterals name="LE_EQUAL" value="3" literal="LE_EQUAL"/>
      <eLiterals name="GREATER" value="4" literal="GREATER"/>
      <eLiterals name="LESSER" value="5" literal="LESSER"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="csp" nsURI="platform:/plugin/org.emoflon.ibex.tgg.core.language/model/Language.ecore#//csp"
      nsPrefix="org.emoflon.ibex.tgg.core.language.csp">
    <eClassifiers xsi:type="ecore:EClass" name="TGGAttributeConstraintDefinition"
        eSuperTypes="#//basic/TGGNamedElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="parameterDefinitions"
          upperBound="-1" eType="#//csp/TGGAttributeConstraintParameterDefinition"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="adornments" upperBound="-1"
          eType="#//csp/TGGAttributeConstraintAdornment" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TGGAttributeConstraintParameterDefinition">
      <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="ecore:EClass ../../org.eclipse.emf.ecore/model/Ecore.ecore#//EDataType"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="index" eType="ecore:EDataType ../../org.eclipse.emf.ecore/model/Ecore.ecore#//EInt"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TGGAttributeConstraintAdornment">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType ../../org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TGGAttributeConstraint">
      <eStructuralFeatures xsi:type="ecore:EReference" name="parameters" upperBound="-1"
          eType="#//csp/TGGAttributeConstraintParameter" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="definition" eType="#//csp/TGGAttributeConstraintDefinition"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TGGAttributeConstraintParameter" abstract="true">
      <eStructuralFeatures xsi:type="ecore:EReference" name="definition" eType="#//csp/TGGAttributeConstraintParameterDefinition"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TGGAttributeVariable" eSuperTypes="#//csp/TGGAttributeConstraintParameter">
      <eStructuralFeatures xsi:type="ecore:EReference" name="node" eType="#//TGGRuleNode"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="attribute" eType="ecore:EClass ../../org.eclipse.emf.ecore/model/Ecore.ecore#//EAttribute"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TGGAttributeLiteral" eSuperTypes="#//csp/TGGAttributeConstraintParameter">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType ../../org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TGGAttributeTempVariable" eSuperTypes="#//csp/TGGAttributeConstraintParameter"/>
  </eSubpackages>
  <eSubpackages name="basic" nsURI="platform:/plugin/org.emoflon.ibex.tgg.core.language/model/Language.ecore#//basic"
      nsPrefix="org.emoflon.ibex.tgg.core.language.basic">
    <eClassifiers xsi:type="ecore:EClass" name="TGGNamedElement">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType ../../org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
    </eClassifiers>
    <eSubpackages name="expressions" nsURI="platform:/plugin/org.emoflon.ibex.tgg.core.language/model/Language.ecore#/misc/expressions"
        nsPrefix="org.emoflon.ibex.tgg.core.language.misc.expressions">
      <eClassifiers xsi:type="ecore:EClass" name="TGGExpression" abstract="true"/>
      <eClassifiers xsi:type="ecore:EClass" name="TGGLiteralExpression" eSuperTypes="#//basic/expressions/TGGExpression">
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="TGGAttributeExpression" eSuperTypes="#//basic/expressions/TGGExpression">
        <eStructuralFeatures xsi:type="ecore:EReference" name="objectVar" eType="#//TGGRuleNode"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="attribute" eType="ecore:EClass ../../org.eclipse.emf.ecore/model/Ecore.ecore#//EAttribute"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="TGGEnumExpression" eSuperTypes="#//basic/expressions/TGGExpression">
        <eStructuralFeatures xsi:type="ecore:EReference" name="eenum" eType="ecore:EClass ../../org.eclipse.emf.ecore/model/Ecore.ecore#//EEnum"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="literal" eType="ecore:EClass ../../org.eclipse.emf.ecore/model/Ecore.ecore#//EEnumLiteral"/>
      </eClassifiers>
    </eSubpackages>
  </eSubpackages>
</ecore:EPackage>
