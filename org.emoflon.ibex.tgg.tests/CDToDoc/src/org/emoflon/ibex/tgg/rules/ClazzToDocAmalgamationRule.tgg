#using CDToDoc.*
#using AttrCondDefLibrary.*

#rule ClazzToDocAmalgamationRule #with CDToDoc

#source { 
	p1:Package{
		-clazzs->subClazz
		-clazzs->superClazz
	}
	
	subClazz:Clazz {
		++ -superClazz->superClazz
	}
	superClazz:Clazz
} 

#target {
	
	f1:Folder{
		-files->subFile
		-files->superFile
	}
	 
	subFile:DocFile {
		++ -allSuperRefs->superFile
		++ -superRef->superFile
	}
	superFile:DocFile 
} 

#correspondence {
	p2f1:PackageToFolder{
		#src->p1
		#trg->f1
	}
	c2f1:ClazzToFile{
		#src->subClazz
		#trg->subFile
	}
	c2f2:ClazzToFile{
		#src->superClazz
		#trg->superFile
	}
}

#attributeConditions {
