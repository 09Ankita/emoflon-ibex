// Add imports here
#import "platform:/plugin/CD/model/CD.ecore"
#import "platform:/plugin/Doc/model/Doc.ecore"

#schema CDToDoc
	
#source {
	CD
}

#target {  
	Doc 
}

#correspondence {
	PackageToFolder{
		#src->Package
		#trg->Folder
	}
	
	PackageToFile{
		#src->Package
		#trg->File
	}
	ClazzToFile{
		#src->Clazz
		#trg->File
	}
}

#attributeConditions {
	#userDefined uppercase(a: EString, b: EString) {
		#sync: [B B], [B F], [F B]
		#gen:  [B B], [B F], [F B], [F F]
	}
	
	// Semantics: summand1 + summand2 == result (where + is addition for Numbers)
	#userDefined add_int(summand1:EInt, summand2:EInt, result:EInt) {
		#sync: [B B B], [B B F], [B F B], [F B B]
		#gen: [B B B], [B B F], [B F B], [F B B], [F F B], [F B F], [B F F]
	}
}
