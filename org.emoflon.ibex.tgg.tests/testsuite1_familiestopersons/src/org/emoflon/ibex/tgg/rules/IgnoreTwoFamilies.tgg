#using testsuite1_familiestopersons.*
#using AttrCondDefLibrary.*

#rule IgnoreTwoFamilies #with testsuite1_familiestopersons

#source { 
	fr:FamilyRegister {
	 	++ -families->f1
	 	++ -families->f2
	}
	
	++ f1:Family {
		name := "Test"
	}
	
	++ f2:Family {
		name := "Test"
	}
}

#target {
	pr:PersonRegister
}

#correspondence {
	fr2pr:RegisterToRegister
	{
		#src->fr
		#trg->pr
	}
}

#attributeConditions {
	
}