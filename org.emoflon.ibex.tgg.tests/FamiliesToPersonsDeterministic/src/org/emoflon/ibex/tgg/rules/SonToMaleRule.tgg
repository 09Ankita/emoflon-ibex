#using FamiliesToPersonsDeterministic.*
#using AttrCondDefLibrary.*

#rule SonToMaleRule #with FamiliesToPersonsDeterministic

#source { 
	f:Family {
		-father->father
		++ -sons->son
	}
	
	father:FamilyMember
	++ son:FamilyMember
}

#target {
	pr:PersonRegister {
		-persons->fp
		++ -persons->p
	}
	fp: Male
	
	++ p:Male
}

#correspondence {
	fa2p:MemberToPerson {
		#src->father
		#trg->fp
	}
	
	++ f2p:MemberToPerson {
		#src->son
		#trg->p
	}
}

#attributeConditions {
	
}
