#using FamiliesToPersonsDeterministic.*
#using AttrCondDefLibrary.*

#abstract #rule SonToMaleRule #with FamiliesToPersonsDeterministic

#source { 
	f:Family {
		name  == "Test"
		++ -sons->son
	}
	
	++ son:FamilyMember
}

#target {
	pr:PersonRegister {
		++ -persons->p
	}
	
	++ p:Male
}

#correspondence {
	++ f2p:MemberToPerson {
		#src->son
		#trg->p
	}
}

#attributeConditions {
	
}
