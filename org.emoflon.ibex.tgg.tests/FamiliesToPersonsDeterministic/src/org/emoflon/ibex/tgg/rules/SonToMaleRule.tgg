#using FamiliesToPersonsDeterministic.*
#using AttrCondDefLibrary.*

#abstract #rule SonToMaleRule #with FamiliesToPersonsDeterministic

#source { 
	f:Family {
		name == "Test"
		postalNumber == 5511
		++ -sons->son
	}
	
	++ son:FamilyMember
}

#target {
	pr:PersonRegister {
		++ -persons->p
	}
	
	++ p:Male
}

#correspondence {
	++ f2p:MemberToPerson {
		#src->son
		#trg->p
	}
}

#attributeConditions {
//	eq_string(f.name, son.name)
//	eq_string(tempVar, son.name)
//	eq_string(tempVar, "Test")
}
