#using FamiliesToPersonsDeterministic.*
#using AttrCondDefLibrary.*

#rule MotherToFemale #with FamiliesToPersonsDeterministic

#source { 
	fr:FamilyRegister {
		++ -families->f
	}
	
	++ f:Family {
		++ -mother->mother
	}
	
	++ mother:FamilyMember
}

#target {
	pr:PersonRegister {
		++ -persons->p
	}
	
	++ p:Female
}

#correspondence {
	f2p:RegisterToRegister {
		#src->fr
		#trg->pr
	}
	
	++ m2p:MemberToPerson {
		#src->mother
		#trg->p
	}
}

#attributeConditions {
	ownConcat(" ", mother.name, f.name, p.name)
}
