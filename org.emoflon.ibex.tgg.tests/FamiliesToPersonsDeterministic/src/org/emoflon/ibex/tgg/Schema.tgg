// Add imports here
#import "platform:/plugin/Families/model/Families.ecore"
#import "platform:/plugin/Persons/model/Persons.ecore"

#schema FamiliesToPersonsDeterministic
	
#source {
	Families
}

#target { 
	Persons	
} 

#correspondence {
	RegisterToRegister {
		#src->FamilyRegister
		#trg->PersonRegister
	}
	
	MemberToPerson {
		#src->FamilyMember
		#trg->Person
	}
}

#attributeConditions {
	#userDefined ownConcat(separator:EString, leftWord:EString, rightWord:EString, result:EString) {
		#sync: [B B B B], [B B B F], [B B F B], [B F F B], [B F B B]
		#gen: [B B B B] , [B B B F], [B B F B], [B F F B], [B F B B], [B F B F], [B B F F], [B F F F]
	}
}
